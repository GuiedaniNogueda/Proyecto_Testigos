<h1>Listing creatives</h1>

<table>
  <thead>
    <tr>
      <th>Creative name</th>
      <th>Order</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @order.creatives.each do |creative| %>
      <tr>
        <td><%= creative.creative_name %></td>
        <td><%= creative.order_id %></td>
        <td><%= link_to 'Mostrar', order_creative_path(creative) %></td>
        <td><%= link_to 'Edit', edit_order_creative_path(creative) %></td>
        <td><%= link_to 'Destroy', order_creative_path(creative), method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Creative', new_order_creative_path(@order) %>
