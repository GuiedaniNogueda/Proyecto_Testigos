<h1>Listing lines</h1>

<table>
  <thead>
    <tr>
      <th>Line number</th>
      <th>Format</th>
      <th>Start date</th>
      <th>End date</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @lines.each do |line| %>
      <tr>
        <td><%= line.line_number %></td>
        <td><%= line.format_id %></td>
        <td><%= line.start_date %></td>
        <td><%= line.end_date %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>